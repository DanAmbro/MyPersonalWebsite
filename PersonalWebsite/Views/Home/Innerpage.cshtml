@{
    ViewData["Title"] = "Innerpage";
}

<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section class="breadcrumbs">
        <div class="container">

            <div class="d-flex justify-content-between align-items-center">
                <h2>Resume Page</h2>
                <ol>
                    <li><a href="@Url.Action("Index","Home")">Home</a></li>
                    <li>Resume Page</li>
                </ol>
            </div>

        </div>
    </section><!-- End Breadcrumbs -->

    <section>
        <div class="col-lg-4 col-md-6 portfolio-item filter-app">
            <div class="portfolio-wrap">
                <embed src="~/forms/Dan Ambro Resume.pdf" type="application/pdf" width="900px" height="900px" />

            </div>
        </div>
    </section>
    <article class="bg-dark p-3">
        <div class="row">
            <div class="col-md-6 offset-md-3 text-center">
                <h4 class="text-white">Guess a number between 1 and 5</h4>
                <div class="form-floating m-3">
                    <input type="number" min="1" max="5" class="form-control" id="number1to5"
                           placeholder="Number (1 - 5)" />
                    <label for="number1to5">number between 1 and 5</label>
                </div>
                <div>
                    <button onclick="guessNumber()" class="btn btn-light m-2">Feeling lucky?</button>
                </div>
                <p id="numberMessage" class="text-white"></p>
            </div>
        </div>
    </article>

</main><!-- End #main -->
